{
	"name": "ants-vscode",
	"displayName": "ants-vscode",
	"description": "VS Code extension for ants",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/dsgsjk/ants-vscode.git"
	},
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ants.helloWorld",
				"title": "ants: Hello World"
			},
			{
				"command": "ants.restart",
				"title": "ants: Restart"
			},
			{
				"command": "ants.init",
				"title": "ants: Init"
			},
			{
				"command": "ants.new",
				"title": "ants: New"
			},
			{
				"command": "ants.filter",
				"title": "ants: Filter"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^10.1.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"@vscode/test-electron": "^2.3.9",
		"vscode-languageclient": "^7.0.0"
	}
}
